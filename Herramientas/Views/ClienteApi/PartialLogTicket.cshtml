@model Herramientas.Models.PartialLogModel

@if (Model != null)
{
    <div class="grid">
        <table cellspacing="0" width="80%">
            <thead>
                <tr>
                    <th>
                        Name
                    </th>
                    <th>
                        Age
                    </th>
                    <th>
                        Address
                    </th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.TicketLogList)
                {
                    <tr>
                        <td align="left">
                            @item.Fecha
                        </td>
                        <td align="left">
                            @item.IdTicket
                        </td>
                        <td align="left">
                            @item.Ip
                        </td>                        
                        <td align="left">
                            @item.Texto
                        </td>                        
                        <td align="left">
                            @item.MasInformacion
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}